{
  "name": "CASTNXT",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@ginkgo-bioworks/react-json-schema-form-builder": "^2.8.0",
    "@material-ui/core": "^4.12.3",
    "@material-ui/data-grid": "^4.0.0-alpha.37",
    "@mui/material": "^5.5.1",
    "@mui/styled-engine-sc": "^5.4.2",
    "@mui/x-date-pickers": "^5.0.8",
    "@rails/webpacker": "5.4.3",
    "@react-pdf/renderer": "^3.1.9",
    "@rjsf/core": "^4.1.1",
    "axios": "^0.26.1",
    "babel-jest": "^29.3.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "bootstrap": "^5.1.3",
    "dayjs": "^1.11.6",
    "html2canvas": "^1.4.1",
    "jest": "^29.3.1",
    "jspdf": "^2.5.1",
    "npm": "^8.5.4",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^17.0.2",
    "react-multi-select-component": "^4.2.4",
    "react-router-dom": "^5.2.0",
    "semantic-ui-react": "^2.1.2",
    "styled-components": "^5.3.3",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ]
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ]
    ]
  },
  "scripts": {
    "test": "jest --updateSnapshot --forceExit",
    "coverage": "jest --collect-coverage --silent --updateSnapshot --forceExit"
  },
  "jest": {
    "roots": [
      "spec/javascript"
    ],
    "testEnvironment": "jsdom",
    "testPathIgnorePatterns": [
      "<rootDir>/spec/javascript/__mocks__",
      "<rootDir>/spec/javascript/__utils__",
      "<rootDir>/node_modules"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/spec/javascript/__mocks__/file.mock.js",
      "\\.(css|less|scss|sss|styl)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "globals": {
      "window": {}
    }
  },
  "devDependencies": {
    "@babel/preset-react": "^7.16.7",
    "jest-css-modules": "^2.1.0",
    "jest-environment-jsdom": "^29.3.1",
    "jest-mock-axios": "^4.7.0-beta",
    "react-test-renderer": "^17.0.2",
    "webpack-dev-server": "^3",
    "yarn": "^1.22.19"
  }
}
